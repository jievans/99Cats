<h1>User Factory</h1>

<h3>Create yourself! (as a user)</h3>

<form action = "<%=users_url%>" method = "post" >
	<input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
	<label for = "user_username">Username</label>
	<input type = "text" name = "user[username]" id = "user_username"><br><br>

	<label for = "user_password">Password</label>
	<input type = "password" name = "user[password]" id = "user_password"><br><br>

	<br><br>
	<input type = "submit" value = "Make my user identity!">

</form>